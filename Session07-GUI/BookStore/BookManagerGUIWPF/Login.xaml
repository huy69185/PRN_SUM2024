<Window x:Class="BookManagerGUIWPF.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookManagerGUIWPF"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="https://www.didongmy.com/vnt_upload/news/05_2024/anh-27-meme-dang-yeu-didongmy.jpg"/>
    </Window.Background>
    <Grid>

        <Button Content="Login" HorizontalAlignment="Left" Margin="277,295,0,0" Click="Button_Click"/>
        <TextBox HorizontalAlignment="Left" Margin="277,162,0,0" TextWrapping="Wrap" Text="UserName" VerticalAlignment="Top" Width="206" Height="45" />
        <TextBox HorizontalAlignment="Left" Margin="277,229,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="206" Height="45"/>

    </Grid>
</Window>
